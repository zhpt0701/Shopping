<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:state_checked="true">
        <shape>
            <solid android:color="@color/green"/>
            <corners android:radius="@dimen/w_san"/>
            <stroke android:color="@color/huibai"
                    android:width="@dimen/biankuangone"/>
        </shape>
    </item>
    <item android:state_selected="true">
        <shape>
            <solid android:color="@color/green"/>
            <corners android:radius="@dimen/w_san"/>
            <stroke android:color="@color/huibai"
                    android:width="@dimen/biankuangone"/>
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape>
            <solid android:color="@color/green"/>
            <corners android:radius="@dimen/w_san"/>
            <stroke android:color="@color/huibai"
                    android:width="@dimen/biankuangone"/>
        </shape>
    </item>
    <item>
        <shape>
            <solid android:color="@color/huibai"/>
            <corners android:radius="@dimen/w_san"/>
            <stroke android:width="@dimen/biankuangone"
                    android:color="@color/huibai"/>
        </shape>
    </item>
</selector>